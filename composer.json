{
    "name": "tmacphp/framework",
    "description": "The Tmac PHP Framework.",
    "keywords": [
        "framework",
        "TmacPHP"
    ],
    "homepage": "http://www.t-mac.org/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "wentmac",
            "email": "wentmac@vip.qq.com"
        }
    ],
    "require": {
        "php": ">=5.1.0",
        "psr/log": "~1.0",
        "psr/container": "~1.0",
        "psr/simple-cache": "^1.0"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "Tmac\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tmac\\tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}